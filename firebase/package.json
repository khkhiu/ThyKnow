{
  "name": "thyknow-express",
  "version": "1.0.0",
  "description": "ThyKnow Telegram bot for self-awareness and connections",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "setup-webhook": "ts-node scripts/setup-webhook.ts",
    "test": "mocha --require ts-node/register test/**/*.test.ts",
    "test:coverage": "nyc --reporter=lcov --reporter=text npm test"
  },
  "keywords": [
    "telegram",
    "bot",
    "journaling",
    "self-awareness",
    "connections"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "agentkeepalive": "^4.6.0",
    "axios": "^1.6.5",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "moment-timezone": "^0.5.43",
    "mongoose": "^8.2.0",
    "node-cron": "^3.0.3",
    "telegraf": "^4.15.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.11",
    "@types/sinon": "^17.0.4",
    "@types/sinon-chai": "^3.2.12",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.4.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.3.0",
    "nodemon": "^3.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.0"
  }
}
